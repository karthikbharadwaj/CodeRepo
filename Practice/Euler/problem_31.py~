'''
problem 31:
    Given 1p, 2p, 5p, 10p, 20p, 50p, £1 (100p) and £2 (200p).
    What are all possible ways to get 2 pounds
'''


def solution(n):

    f = {}
    coins = [1, 2, 5, 10, 20, 50, 100, 200]
    f[0] = 1
    f[1] = coins[0]

    for index,coin in  enumerate(coins):
        for i in range(coin, n + 1):
            f[index] += f.get(i - coin, 0)

        f[i] =  total

    return f[n]

print solution(5)





