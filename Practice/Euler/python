from general import factors

def get_triangle_num(n):
    
    triangle_numbers = set()
    for i in range(n,0,-1):
        yield (i*(i+1))//2
  

def solution():

    ''' find the first triangle number to have 500 factors
    '''

    i = 1
    t_num = 1
    triangle_nums = get_triangle_num(1000)
    print triangle_nums

print solution()
